{
  "name": "bull-board",
  "description": "Bull queue UI for inspecting jobs",
  "keywords": [
    "bull",
    "redis",
    "queue"
  ],
  "main": "dist/index.js",
  "private": false,
  "version": "0.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vcapretz/bull-board.git"
  },
  "bugs": {
    "url": "https://github.com/vcapretz/bull-board/issues"
  },
  "scripts": {
    "prepublishOnly": "yarn build:clean && yarn build",
    "prepare": "yarn build",
    "build": "yarn build:clean && yarn build:ui && yarn build:routes",
    "build:ui": "NODE_ENV=production webpack",
    "build:routes": "yarn tsc",
    "build:watch": "NODE_ENV=production webpack --watch",
    "build:clean": "rm -rf ./static ./dist",
    "lint": "prettier --check \"./**/*.js\" && eslint --ext .js .",
    "start": "node example.js",
    "start:example": "yarn build && yarn start"
  },
  "dependencies": {
    "date-fns": "2.9.0",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "pretty-bytes": "5.3.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-highlight": "0.12.0"
  },
  "devDependencies": {
    "@types/bull": "^3.12.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@typescript-eslint/typescript-estree": "^2.17.0",
    "babel-preset-react-app": "^7.0.2",
    "bull": "^3.12.1",
    "bullmq": "^1.6.6",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-dev-utils": "^8.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
