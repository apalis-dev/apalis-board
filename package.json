{
  "name": "bull-board",
  "description": "Bull queue UI for inspecting jobs",
  "keywords": [
    "bull",
    "redis",
    "queue"
  ],
  "main": "index.js",
  "private": false,
  "version": "0.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vcapretz/bull-board.git"
  },
  "bugs": {
    "url": "https://github.com/vcapretz/bull-board/issues"
  },
  "scripts": {
    "prepublishOnly": "rm -rf ./build && yarn build",
    "prepare": "yarn build",
    "build": "NODE_ENV=production webpack",
    "build:watch": "NODE_ENV=production webpack --watch",
    "lint": "prettier --check \"./**/*.js\" && eslint --ext .js .",
    "start": "node example.js",
    "start:example": "yarn build && yarn start"
  },
  "dependencies": {
    "date-fns": "2.9.0",
    "ejs": "^2.6.1",
    "express": "^4.15.2",
    "pretty-bytes": "^5.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-highlight": "^0.12.0"
  },
  "devDependencies": {
    "babel-preset-react-app": "^7.0.2",
    "bull": "^3.12.1",
    "bullmq": "^1.6.6",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.0",
    "prettier": "^1.19.1",
    "react-dev-utils": "^8.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
